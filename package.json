{
  "name": "react-kickstart",
  "version": "1.0.0",
  "description": "React for Kickstart",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "serve",
    "build": "browserify src/**/*.{jsx,js} -d -o dst/bundle.js -t babelify",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": ["src/**"],
      "extensions": "js,jsx"
    }
  },
  "repository": {
    "type": "git",
    "url": "react-kickstart"
  },
  "keywords": [
    "React",
    "Kickstart",
    "Wix"
  ],
  "author": "Dan Shappir",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "eslint": "^2.0.0",
    "eslint-config-wix-editor": "^0.1.1",
    "eslint-plugin-lodash": "^1.0.5",
    "grunt": "^0.4.5",
    "grunt-contrib-csslint": "^0.5.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-eslint": "^18.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "dependencies": {
    "lodash": "^4.3.0"
  }
}
